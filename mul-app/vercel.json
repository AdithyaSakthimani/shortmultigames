{
    "redirects": [
      {
        "source": "/:path*",
        "destination": "https://www.duonet.in/:path*",
        "permanent": true
      },
      {
        "source": "http://duonet.in",
        "destination": "https://www.duonet.in",
        "permanent": true
      },
      {
        "source": "http://www.duonet.in",
        "destination": "https://www.duonet.in",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Robots-Tag",
            "value": "index, follow"
          },
          {
            "key": "Strict-Transport-Security",
            "value": "max-age=63072000; includeSubDomains; preload"
          }
        ]
      }
    ]
  }